{
    "name": "kak/ckeditor",
    "description": "Yii2 summernote widget",
    "type": "yii2-extension",
	"homepage": "https://github.com/sanchezzzhak/kak-ckeditor",
    "keywords": ["yii2","ckeditor","ckeditor for yii2" , "widget", "wysiwyg"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tutik Alexsandr",
            "email": "sanchezzzhak@ya.ru"
        }
    ],
	"require": {
		"php":">=5.4.0",
        "yiisoft/yii2-imagine": "*",
        "ckeditor/ckeditor": "stable"
	},
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
	"autoload": {
		"psr-4": {
			"kak\\widgets\\ckeditor\\": ""
		}
	}
}