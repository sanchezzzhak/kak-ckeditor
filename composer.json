{
    "name": "kak/widgets/summernote",
    "description": "Yii2 summernote widget",
    "type": "yii2-extension",
	"homepage": "https://github.com/sanchezzzhak/kak-summernote",
    "keywords": ["yii2","summernote","summernote for yii2" , "widget", "wysiwyg"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tutik Alexsandr",
            "email": "sanchezzzhak@ya.ru"
        }
    ],
	"require": {
		"php":">=5.4.0",
        "bower-asset/font-awesome": "4.*@stable",
        "bower-asset/codemirror": "5.*@stable",
        "bower-asset/summernote": "0.*@stable"
	},
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
	"autoload": {
		"psr-4": {
			"kak\\widgets\\summernote\\": ""
		}
	}
}